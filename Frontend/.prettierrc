{
    "plugins": ["prettier-plugin-organize-imports"],
    "singleQuote": true,
    "printWidth": 80,
    "proseWrap": "always",
    "tabWidth": 4,
    "useTabs": false,
    "trailingComma": "none",
    "bracketSpacing": true,
    "semi": true,
    "organizeImportsSkipDestructiveCodeActions": true,
    "htmlWhitespaceSensitivity": "strict"
}